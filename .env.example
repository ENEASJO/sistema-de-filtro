# Puerto del servidor
PORT=3000

# Modo de producción (Railway lo configura automáticamente)
NODE_ENV=production

# Timeout de requests en milisegundos (4 minutos)
REQUEST_TIMEOUT=240000

# Máximo de RUCs por batch (Railway gratuito tiene límites)
MAX_RUCS_PER_BATCH=3

# Variables de Playwright para producción
PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=0
PLAYWRIGHT_BROWSERS_PATH=/root/.cache/ms-playwright
